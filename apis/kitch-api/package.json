{
  "name": "kitch-api",
  "version": "1.0.0",
  "description": "hello world sample for NodeJS",
  "main": "app.js",
  "repository": "https://github.com/crudyapps/open-kitch.git",
  "author": "SAM CLI",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.713.0",
    "axios": "^0.18.0"
  },
  "scripts": {
    "clean": "rm -rf ../.aws-sam && rm -rf dist",
    "build-sam": "cd .. && sam build",
    "build-src": "tsc src/*.ts -outDir dist",
    "build": "npm run clean && npm run build-src && gulp && npm run build-sam",
    "echo": "cd .. && env-cmd -f ./local.env echo DynamoDbEndpoint=$DYNAMODB_ENDPOINT WebClientOrigin=$WEB_CLIENT_ORIGIN",
    "start-api": "cd .. && ./start-api",
    "start-dyndb": "docker run -d --restart unless-stopped --name kitch-db -p 8000:8000 amazon/dynamodb-local:latest",
    "start-dyndb-admin": "DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.59",
    "@types/node": "^14.0.22",
    "chai": "^4.2.0",
    "dynamodb-admin": "^4.0.0",
    "gulp": "^4.0.2",
    "mocha": "^6.1.4",
    "typescript": "^3.9.6"
  }
}
