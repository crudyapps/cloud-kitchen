{
  "name": "open-kitch-ui",
  "version": "1.0.0",
  "description": "The user interface for the kitchen",
  "repository": {
    "type": "git",
    "url": "https://github.com/crudyapps/open-kitch.git"
  },
  "main": "App.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "clean": "rm -rf dist .cache",
    "build-prod": "npm run clean && NODE_ENV=production parcel build ./src/index.html --no-minify",
    "loc-to-prod": "npm run build-prod && env-cmd -f ./.local-to-prod wrangler publish"
  },
  "author": "crudyapps",
  "license": "ISC",
  "devDependencies": {
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "env-cmd": "^10.1.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "0.0.11",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-charts": "^3.0.15",
    "react-router-dom": "^5.2.0"
  },
  "staticFiles": {
    "staticPath": "src/public",
    "watcherGlob": "src/public/**"
  }
}
